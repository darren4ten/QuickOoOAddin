<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab idMso="TabMail">
        <group   id="HomeGroup" label="Content">
          <button id="btnOoO" label="OoO"
                  screentip="Text" onAction="OnBtnOoO"
                  supertip="Create a new OoO Meeting request."/>
          <button id="btnWFH" label="WFH"
                  screentip="Table" onAction="OnBtnWFH"
                  supertip="Create a new WFH Meeting request."/>

          <button id="btnConfig" label="Config Templates"
                  screentip="Table" onAction="OnConfigTemplates"
                  supertip="Config the OoO and WFH meeting request templates."/>
        </group>
      </tab>
    </tabs>

  </ribbon>
